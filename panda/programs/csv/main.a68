PR include "csv.a68" PR;

FILE f;
INT err = open(f, "./tasks.csv", stand in channel);

IF err = 0 THEN
	REGS regs := read csv(f);

	FOR i TO UPB regs DO
	  printf(($g(-0)": "$, i));

		FOR j TO UPB regs[i] DO
			printf(($g(-0)":"g"; "$, j, regs[i][j]))
		OD;

		print(new line)
	OD;

	close(f)
ELSE
	printf(($"Error: "gl$, err))
FI
